{
    "name": "lawniczek-piotr/isolate-messenger-bridge",
    "description": "Integrates Isolate with Symfony Messenger component (via Middleware)",
    "keywords": [
        "isolate",
        "symfony",
        "messenger",
        "command bus"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr ≈Åawniczek",
            "email": "lawniczek.piotr@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "isolate/isolate": "1.0.*@dev",
        "isolate/unit-of-work": "1.0.*@dev",
        "isolate/lazy-objects": "1.0.*@dev",
        "symfony/messenger": "^4.2@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1@dev"
    },
    "autoload": {
        "psr-0": {
            "Isolate": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Isolate\\Tests": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
